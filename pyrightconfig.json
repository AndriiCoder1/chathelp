{
    "venvPath": ".",
    "venv": "venv",
    "pythonPath": "./venv/Scripts/python.exe",
    "analysis": {
        "autoImportCompletions": true,
        "useLibraryCodeForTypes": true,
        "diagnosticMode": "workspace",
        "typeCheckingMode": "basic",
        "reportMissingModuleSource": false,
        "reportAttributeAccessIssue": "none",
        "ignore": [
            "openai",
            "pyttsx3"
        ]
    },
    "include": [
        "*.py"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        ".git"
    ]
}